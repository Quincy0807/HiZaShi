def shared_pods
  pod 'RealmSwift'
end

target 'HiZashi' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  platform :ios, '9.0'
  use_frameworks!

  shared_pods

end

target 'HiZashi WatchKit App' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  platform :watchos, '5'
  use_frameworks!

  shared_pods

end

target 'HiZashi WatchKit Extension' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  platform :watchos, '5'
  use_frameworks!

  shared_pods

end
